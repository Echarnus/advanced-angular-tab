{
  "name": "federated-shell",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc -b --noCheck && vite build",
    "build:all": "cd ../ && npm run build && cd angular && npm run build && cd ../federated && npm run build",
    "dev:all": "concurrently \"cd ../ && npm run dev\" \"cd ../angular && npm start\" \"npm run dev\" --names \"React,Angular,Shell\" --prefix-colors \"blue,red,green\"",
    "lint": "eslint .",
    "preview": "vite preview --port 3000"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "clsx": "^2.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.3.6",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.28.0",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.7.2",
    "vite": "^6.3.5"
  }
}
